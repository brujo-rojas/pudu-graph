{
  "name": "pudu-graph",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/pudu-graph.js",
  "module": "dist/pudu-graph.js",
  "types": "dist/pudu-graph.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run copy-react-wrapper",
    "copy-react-wrapper": "mkdir -p dist/react && cp src/react/PuduGraphReact.tsx dist/react/PuduGraphReact.tsx",
    "preview": "vite preview"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/pudu-graph.js",
      "require": "./dist/pudu-graph.js",
      "types": "./dist/pudu-graph.d.ts"
    },
    "./react/PuduGraphReact": {
      "import": "./dist/react/PuduGraphReact.js",
      "require": "./dist/react/PuduGraphReact.js"
    }
  },
  "dependencies": {
    "@lit-labs/react": "^2.1.3",
    "@reduxjs/toolkit": "^2.8.2",
    "lit": "^3.3.0",
    "pwa-helpers": "^0.9.1",
    "redux": "^5.0.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "sass": "^1.89.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "dist",
    "emitDeclarationOnly": false,
    "rootDir": "src"
  },
  "include": [
    "src"
  ]
}